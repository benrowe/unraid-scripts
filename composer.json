{
    "name": "benrowe/unraid-scripts",
    "description": "unraid scripts",
    "license": "MIT",
    "require": {
        "hassankhan/config": "^1.0",
        "koriym/psr4list": "^1.0",
        "shrikeh/teapot": "^2.3",
        "symfony/console": "^3.2",
        "symfony/http-foundation": "^4.0",
        "symfony/yaml": "^4.0",
        "vlucas/phpdotenv": "^2.4",
        "wikimedia/composer-merge-plugin": "^1.4"
    },
    "autoload": {
      "psr-4": {
        "App\\": "src/",
        "App\\Commands\\": "data/commands"
      }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "merge-plugin": {
            "include": [
                "data/composer.json"
            ]
        }
    }
}
